<!--Build-->
<div id="build_@Model.Match.GameId" style="display: none;">
    <div>
        <!--Timeline-->
        <div style="margin-bottom: 20px;">
            <h4>Items Purchase Timeline</h4>
            <hr />
        </div>
        <div>
            <ul>
                @{ var firstDone = false; }
                @foreach (var itemPurchaseEvents in Model.Timeline.ItemPurchaseEventsByTimestamp)
                {
                    if (itemPurchaseEvents.Value.Count == 0)
                    {
                        continue;
                    }
                    long firstTimestamp = 0;
                    <li class="timeline-item">
                        @if (firstDone == true)
                        {
                            <div class="timeline-arrow">
                                <i class="fa fa-angle-right fa-lg" aria-hidden="true"></i>
                            </div>
                        }
                        @foreach (var itemPurchase in itemPurchaseEvents.Value)
                        {
                            if (firstTimestamp == 0)
                            {
                                firstTimestamp = itemPurchase.Timestamp;
                            }
                            <img src="@itemPurchase.ItemUrl" height="40" width="40" />
                        }
                        <div style="text-align: center; margin-right: @(firstDone == true ? "-30px" : "0");">
                            @Math.Round(TimeSpan.FromMilliseconds(firstTimestamp).TotalMinutes, 0) min
                        </div>
                        @{ firstDone = true; }
                    </li>
                }
            </ul>
        </div>
        
    </div>
    <!--Skill points table-->
    <div style="margin-bottom: 20px;">
        <h4>Skill Points Spent</h4>
        <hr />
    </div>
    <div class="col-md-12">
        <div class="row">
            @{
                var qUrl = "";
                var wUrl = "";
                var eUrl = "";
                var rUrl = "";

                foreach (var spell in Model.Spells)
                {
                    if (Model.Spells.IndexOf(spell) == 0) // Q
                    {
                        qUrl = spell.IconFullUrl;
                    }
                    else if (Model.Spells.IndexOf(spell) == 1) // W
                    {
                        wUrl = spell.IconFullUrl;
                    }
                    else if (Model.Spells.IndexOf(spell) == 2) // E
                    {
                        eUrl = spell.IconFullUrl;
                    }
                    else if (Model.Spells.IndexOf(spell) == 3) // R
                    {
                        rUrl = spell.IconFullUrl;
                    }
                }
            }
            <div class="col-md-12">
                <table id="skillsTable_@Model.Match.GameId" class="skills-table">
                    <tbody>
                        <tr id="QRow_@Model.Match.GameId">
                            <td class="skills-table-first-td">Q</td>
                            <td class="skills-table-img-td"><img src="@qUrl" class="skills-table-img" /></td>
                            @foreach (var skillPoint in Model.Timeline.SkillEvents)
                            {
                                if (skillPoint.SkillSlot == 1)
                                {
